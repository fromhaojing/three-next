{
  "name": "r3f-next-starter",
  "version": "2.0.0",
  "description": "A production-ready starter template for building immersive 3D web experiences with React Three Fiber, Next.js 16, and TypeScript.",
  "authors": [
    "Renaud ROHLINGER <https://twitter.com/onirenaud>"
  ],
  "license": "MIT",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22.x"
  },
  "keywords": [
    "react",
    "three.js",
    "react-three-fiber",
    "r3f",
    "next.js",
    "3d",
    "webgl",
    "starter",
    "template"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/r3f-next-starter"
  },
  "dependencies": {
    "@react-spring/three": "^10.0.3",
    "@react-spring/web": "^10.0.3",
    "@react-three/drei": "^10.7.7",
